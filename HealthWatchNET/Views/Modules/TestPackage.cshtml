@model HealthWatchNET.Table
@{
    ViewBag.Title = "TestPackage";
    Layout = "~/Views/Modules/ModuleLayout.cshtml";
}

<table cellpadding="0" cellspacing="0" border="0" width="99.5%" class="cell_table">
<tr class="cell_tr">
    <td class="cell_th" width="30">#</td>
    <td class="cell_th" width="80">패키지코드</td>
    <td class="cell_th_rt">패키지명</td>
</tr>
@{
    Dictionary<string, object> ret = new Dictionary<string, object>();
    int cnt = 0;
}
@while (Model.FetchTo(out ret) == true)
{
    cnt++;
<tr class="cell_tr">
    <td class="cell_td" width="30">@cnt</td>
    <td class="cell_td" width="80">@ret["PKGCODE"]</td>
    <td class="cell_td"><a href="javascript:parent.loadItemByPkgCode('@ret["PKGCODE"]')">@ret["PKGNAME"]</a></td>
</tr>
}
</table>
